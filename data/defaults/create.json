{
  "summary": "Create a new <%- name %>",
  "description": "",
  "operationId": "<%- name %>.create",
  "parameters": [
    {
      "description": "The <%- name %> to be created, minus the unique identifier (that will be generated by the server) and the metatadata",
      "name": "body",
      "in": "body",
      "required": true,
      "schema": <%- JSON.stringify(schema) %>
    }
  ],
  "responses": {
    "201": {
      "description": "<%- name %> successfully created",
      "schema": <%- JSON.stringify(schema) %>,
      "headers": {
        "Location": {
          "description": "URI of the newly created resource",
          "type": "string",
          "format": "uri"
        }
      }
    },
    "default": { "$ref": "#/responses/defaultError" }
  }
}